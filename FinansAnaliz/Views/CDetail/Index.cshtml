@using Microsoft.AspNetCore.Identity
@inject SignInManager<IdentityUser> SignInManager
@inject UserManager<IdentityUser> UserManager
@inject FinansAnaliz.Data.AppDbContext _appDbContext
@{
    Layout = null;
    var user = _appDbContext.AppUsers.FirstOrDefault(x => x.UserName == SignInManager.UserManager.GetUserName(User));
    var data = _appDbContext.EntryDates.Where(x => x.CompanyName == user.CompanyName).Any();
    List<FinansAnaliz.Models.Rasyo> rasyo = ViewBag.Rasyo is 0 ? new List<Rasyo>() { new Rasyo() { CompanyName = user.CompanyName } } :ViewBag.Rasyo;
    List<Dictionary<string, string>> aktif = ViewBag.aktif is 0 ? new List<Dictionary<string, string>>() { } : ViewBag.aktif;
    List<DateTime> entryDates = ViewBag.entryDates is 0 ? new List<DateTime>() { } : ViewBag.entryDates;
    List<RuleProblemCompany> ProblemCompanyList = ViewBag.ProblemCompanyList is 0 ? new List<RuleProblemCompany>() { } : ViewBag.ProblemCompanyList;
    List<int> RuleIdList = ViewBag.RuleIdList is 0?new List<int>(){}:ViewBag.RuleIdList;

    

    var ruleCount = _appDbContext.ruleProblemCompanies.Where(x => x.IsActive == true).Where(x => x.RegisterCompany == user.CompanyName).Select(x => x.RuleId).Distinct().ToList();

    var showBooks = _appDbContext.Settings.Where(x => x.CompanyName == user.CompanyName).Select(x => x.IsClientShowBook).FirstOrDefault();

    int t = 0;
    int r = 0;
}


