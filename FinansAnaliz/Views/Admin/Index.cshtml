@{
    Layout = null;
}
@using Microsoft.AspNetCore.Identity
@using System.Globalization
@inject SignInManager<IdentityUser> SignInManager
@inject UserManager<IdentityUser> UserManager
@inject FinansAnaliz.Data.AppDbContext _appDbContext

@{

    List<FinansAnaliz.Models.Rasyo> Rasyo = ViewBag.Rasyo;
    IEnumerable<FinansAnaliz.Models.TekliVeri> Tekli = ViewBag.Tekli;
    IEnumerable<string> Firmalar = ViewBag.Firma;
    var mainCode = _appDbContext.AppUsers.Where(x => x.UserName == User.Identity.Name).Select(x => x.MainCompanyCode).FirstOrDefault();

    var entryDates = _appDbContext.EntryDates.Where(x=>x.MainCompanyCode == mainCode).GroupBy(x => x.CompanyName).Select(g => new { CompanyName = g.Key, EntryDate = g.Max(x => x.DateEntry) }).ToList();
    var AlacaklarinOrtalamaTahsilSuresi = (from rasyo in Rasyo.AsParallel() orderby rasyo.AlacaklarinOrtalamaTahsilSuresi descending select rasyo).FirstOrDefault();
    var FinansalKaldirac = (from rasyo in Rasyo.AsParallel() orderby rasyo.FinansalKaldirac descending select rasyo).FirstOrDefault();
    var FaaliyetKarliligi = (from rasyo in Rasyo.AsParallel() orderby rasyo.FaaliyetKarliligi descending select rasyo).FirstOrDefault();
    var NakitOrani = (from rasyo in Rasyo.AsParallel() orderby rasyo.NakitOrani descending select rasyo).FirstOrDefault();
    CurrencyRate rate = ViewBag.rate;
    //var SelectRasyo = (from rasyo in Rasyo.AsParallel() join tekli in Tekli.AsParallel() on rasyo.CompanyName equals tekli.CompanyName join dates in entryDates.AsParallel() on rasyo.CompanyName equals dates.CompanyName where rasyo.Tarih == "2022-06-30" select new { rasyo, firmaSkoru = tekli.FirmaSkoru }).ToList();
    var SelectRasyo = (from rasyo in Rasyo.AsParallel() join tekli in Tekli.AsParallel() on rasyo.CompanyName equals tekli.CompanyName join dates in entryDates.AsParallel() on rasyo.CompanyName equals dates.CompanyName select new { rasyo, firmaSkoru = tekli.FirmaSkoru }).ToList();
    var dupontAnaliz = (from tekli in Tekli.AsParallel() orderby tekli.Dupond_Analizi descending select tekli).FirstOrDefault();
    var altmanZUretim = (from tekli in Tekli.AsParallel() orderby tekli.Altman_Z_Uretim descending select tekli).FirstOrDefault();
    var altmanZHizmet = (from tekli in Tekli.AsParallel() orderby tekli.Altman_Z_Hizmet descending select tekli).FirstOrDefault();

    CurrencyRate CurrencyRate = ViewBag.currencyRate;
    var companyCount = _appDbContext.ruleProblemCompanies.AsParallel().Where(x => x.IsActive == true).Where(x => x.ProblemCompanyName != x.RegisterCompany).Select(x => x.ProblemCompanyName).Distinct().ToList();
    var ruleCount = _appDbContext.ruleProblemCompanies.AsParallel().Where(x => x.IsActive == true).Where(x => x.ProblemCompanyName != x.RegisterCompany).Select(x => x.RuleId).Distinct().ToList();
}

